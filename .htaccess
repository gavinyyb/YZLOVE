RewriteRule ^group/read(\d+)\.html /group/read\.php\?fid=$1
RewriteRule ^group/partyshow(\d+)\.html /group/partyshow\.php\?fid=$1






RewriteRule ^home/(\d+) /home/main\.php\?uid=$1
